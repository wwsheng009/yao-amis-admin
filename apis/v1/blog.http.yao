{
    "name": "amis的请求",
    "version": "1.0.0",
    "description": "amis集成请求",
    "group": "",
    "guard": "-",
    "paths": [
        {
            // /api/v1/blog/web/:blog
            "label": "MetaWeblog",
            "description": "MetaWeblog Api",
            "path": "/web/:blog",
            "method": "POST",
            "guard": "-",
            "process": "scripts.blog.web.metaWeblogHandler",
            "in": [
                "$param.blog",
                ":query",
                ":body"
            ],
            "out": {
                "status": 200,
                "body": "{{content}}",
                "type": "text/xml"
            }
        }
    ]
}